parse=true
mark_oovs=false
pop-limit=1000
useCubePrune=false
useBeamAndThresholdPrune=false

#lm_file=/home/hltcoe/ccallison/parallel_corpus/model/lm/a2e.giga.noUN.5gram.lm.gz
lm_file=test.lm

