<suite name="TestSuite"    verbose="1" >

  <test name="Lattice" >
    <classes>
       <class name="joshua.lattice.NodeTest" />    
       <class name="joshua.lattice.ArcTest"  />
       <class name="joshua.lattice.LatticeTest" />
    </classes>
  </test>

  <test name="Pattern" >
    <classes>
       <class name="edu.jhu.sa.util.suffix_array.PatternTest" />    
    </classes>
  </test>

  <test name="PrefixTree" >
    <classes>
       <class name="edu.jhu.sa.util.suffix_array.PrefixTreeTest" />
       <class name="edu.jhu.sa.util.suffix_array.PrefixTreeAdvancedTest" />
       <class name="joshua.util.lexprob.LexProbsTest" />    
    </classes>
  </test>
 
  
    <test name="Vocab" >
    <classes>
       <class name="joshua.util.sentence.VocabularyTest" />  
       <class name="edu.jhu.sa.util.suffix_array.CorpusArrayTest" />  
    </classes>
  </test>


  <test name="SuffixArray" >
    <classes>
       <class name="edu.jhu.sa.util.suffix_array.SuffixArrayTest" /> 
       <class name="edu.jhu.sa.util.suffix_array.ExtractRules" />    
    </classes>
  </test>

</suite>
